     db:
        security_opt:
          - "apparmor:db_profile"
        container_name: db
        image: mariadb #:10
        command: --transaction-isolation=READ-COMMITTED --binlog-format=ROW
        restart: always
        volumes:
                - db_:/var/lib/mysql
        environment:
                - MYSQL_HOST=db
                - MYSQL_ROOT_PASSWORD=secret
                - MYSQL_PASSWORD=secret
                - MYSQL_DATABASE=nextcloud
                - MYSQL_USER=nextcloud

