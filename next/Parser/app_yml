     app:
        security_opt:
          - "apparmor:app_profile"
        image: nextcloud:latest
        container_name: nextcloud_securewilly
        ports:
                - 8080:80
        links:
                - db
        volumes:
                - nextcloud:/var/www/html
                - /home/ubuntu/SecureWilly/next/data:/var/www/html/data
               
        environment:
                - NEXTCLOUD_ADMIN_USER=willy
                - NEXTCLOUD_ADMIN_PASSWORD=secret
                - NEXTCLOUD_TABLE_PREFIX=nc_
                - NEXTCLOUD_DATA_DIR=/var/www/html/data
        restart: always

