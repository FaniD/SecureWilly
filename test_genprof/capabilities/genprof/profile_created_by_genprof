  /bin/chown rix,
  /bin/dash ix,
  /bin/grep rix,
  /dev/pts/2 r,
  /etc/.pwd.lock w,
  /etc/default/useradd r,
  /etc/group r,
  /etc/login.defs r,
  /etc/nsswitch.conf r,
  /etc/passwd r,
  /home/*/Security-on-Docker/test_genprof/capabilities/genprof/ r,
  /home/*/Security-on-Docker/test_genprof/capabilities/genprof/hello rw,
  /home/ubuntu/Security-on-Docker/test_genprof/capabilities/genprof/chown_it.sh r,
  /proc/filesystems r,
  /proc/sys/kernel/ngroups_max r,
  /usr/sbin/useradd rix,

}
